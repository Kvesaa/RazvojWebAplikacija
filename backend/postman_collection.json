	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Create Sanctum Token",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Accept",
								"value": "application/json"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"reader@example.com\",\n    \"password\": \"password\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/token",
							"host": ["{{base_url}}"],
							"path": ["api", "token"]
						},
						"description": "Exchange session for Sanctum token"
					},
					"response": []
				},
				{
					"name": "Revoke Sanctum Token",
					"request": {
						"method": "DELETE",
						"header": [
							{
								"key": "Accept",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{sanctum_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/token",
							"host": ["{{base_url}}"],
							"path": ["api", "token"]
						},
						"description": "Revoke current Sanctum token"
					},
					"response": []
				}
			]
		},
